{
	"root": true,
	"parserOptions": {
		"ecmaVersion": 6
	},
	"env": {
		"node": true,
	},
	"rules": {
		"no-undefined": "warning",
		"no-self-compare": "warning",
		"no-with": "error",
		"yoda": "warning",
		"comma-spacing": ["warning", {"before": false, "after": true}],
		"no-invalid-this": "error",
		"no-multiple-empty-lines": ["warning", {"max": 2, "maxEOF": 1}],
		"no-mixed-spaces-and-tabs": ["error", "smart-tabs"]
	}
}